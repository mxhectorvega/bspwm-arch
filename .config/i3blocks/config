[CPU]
full_text= 🔥 91.0% @ 89.2°C
command=~/.config/i3blocks/scr/cpu
interval=30
color=#00B4EB

[MEM]
command=free -h | awk '/Mem:/ { printf(" 🐏 %5s/%s \n", $3, $2)}'
interval=15
color=#FEC925

[SOUND]
full_text= 🔇:  0% 
command=~/.config/i3blocks/scr/sonido
interval=5
signal=10

[TIME_DATE]
command=date +"📅 %a, %d %b - %H:%M"
interval=60
